    <DataSet Name="KeyProtectorsLookup">
      <Query>
        <DataSourceName>SCCM_DB</DataSourceName>
        <CommandText>
        SELECT N'0' [KeyProtectorId], N'protectorTypeUnknown' [KeyProtectorName]
        UNION
        SELECT N'1' [KeyProtectorId], N'protectorTypeTPM' [KeyProtectorName]
        UNION
        SELECT N'2' [KeyProtectorId], N'protectorTypeExternalKey' [KeyProtectorName]
        UNION
        SELECT N'3' [KeyProtectorId], N'protectorTypeNumericPassword' [KeyProtectorName]
        UNION
        SELECT N'4' [KeyProtectorId], N'protectorTypeTPMPIN' [KeyProtectorName]
        UNION
        SELECT N'5' [KeyProtectorId], N'protectorTypeTPMUSBStartupKey' [KeyProtectorName]
        UNION
        SELECT N'6' [KeyProtectorId], N'protectorTypeTPMPINUSBStartupKey' [KeyProtectorName]
        UNION
        SELECT N'7' [KeyProtectorId], N'protectorTypePublicKey' [KeyProtectorName]
        UNION
        SELECT N'8' [KeyProtectorId], N'protectorTypePassword' [KeyProtectorName]
        UNION
        SELECT N'9' [KeyProtectorId], N'ProtectorTPMCertificate' [KeyProtectorName]
        UNION
        SELECT N'10' [KeyProtectorId], N'ProtectorSID' [KeyProtectorName]
        UNION
        SELECT N'200' [KeyProtectorId], N'ProtectorDRA' [KeyProtectorName]
        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
