  <Code>
Public Function TrimPrefixComma( ByVal str As String ) As String
        Dim RemoveChar() As Char = {","}
        Return str.TrimStart(RemoveChar)
End Function

Public Function EncodeString(ByVal toEncode As String) As String
        On Error GoTo EncodeError
        Dim bytesToEncode As Byte() = System.Text.UnicodeEncoding.Unicode.GetBytes(toEncode.ToUpper())
        Dim encodedValue As String = System.Convert.ToBase64String(bytesToEncode)
        Return encodedValue
EncodeError:
        Return ""
End Function

Public Function DecodeString(ByVal toDecode As String) As String
        On Error GoTo ErrorExit
        Dim decodedBytes As Byte() = System.Convert.FromBase64String(toDecode)
        Dim decodedValue As String = System.Text.UnicodeEncoding.Unicode.GetString(decodedBytes)
        Return decodedValue
ErrorExit:
        Return ""
End Function

Public Function ToLocalTime(ByVal utcTime As DateTime) As DateTime
        On Error GoTo ErrorExit
        Return utcTime.ToLocalTime()
ErrorExit:
        Return utcTime
End Function
