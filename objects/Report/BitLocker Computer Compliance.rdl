    <DataSet Name="VolumeDetails">
      <Query>
        <DataSourceName>SCCM_DB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@encodedComputerNameParameter">
            <Value>=Code.EncodeString(Parameters!computerNameParameter.Value)</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          SELECT BitlockerDetails.DriveLetter0 [DriveLetter],
            ComputerSystem.Name0 [ComputerName],
            ComputerSystem.Domain0 [DomainName],
            BitlockerDetails.MbamVolumeType0 [MbamVolumeType],
            BitlockerDetails.EncryptionMethod0 [EncryptionMethod],
            BitlockerDetails.ConversionStatus0 [ConversionStatus],
            BitlockerDetails.ProtectionStatus0 [ProtectionStatus],
            BitlockerDetails.IsAutoUnlockEnabled0 [IsAutoUnlockEnabled],
            BitlockerDetails.KeyProtectorTypes0 [KeyProtectorTypes]
          FROM v_GS_BITLOCKER_DETAILS BitlockerDetails
            INNER JOIN v_R_System_Valid RSystem 
              ON BitlockerDetails.ResourceID = RSystem.ResourceID
            INNER JOIN v_GS_COMPUTER_SYSTEM ComputerSystem
              ON RSystem.ResourceID = ComputerSystem.ResourceID
            LEFT OUTER JOIN v_GS_MBAM_POLICY MBAMPolicy
              ON MBAMPolicy.ResourceID = RSystem.ResourceID
          WHERE (MBAMPolicy.EncodedComputerName0 = @encodedComputerNameParameter)
            AND (BitlockerDetails.MbamVolumeType0 = 1 OR BitlockerDetails.MbamVolumeType0 = 2)
          ORDER BY DomainName, ComputerName, DriveLetter 
        </CommandText>
      </Query>
      <Fields>
        <Field Name="ComputerName">
          <DataField>ComputerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DomainName">
          <DataField>DomainName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DriveLetter">
