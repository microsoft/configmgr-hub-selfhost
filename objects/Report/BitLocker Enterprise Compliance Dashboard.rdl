    <DataSet Name="InventoryViewExistence">
      <Query>
        <DataSourceName>SCCM_DB</DataSourceName>
        <CommandText>
      DECLARE @query nvarchar(70)
          SET @query = N'SELECT COUNT(EncodedComputerName0) AS Status FROM V_GS_MBAM_POLICY'
      BEGIN TRY
        EXEC(@query)
      END TRY
      BEGIN CATCH
        SELECT 0 AS Status
      END CATCH
    </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ComplianceDetails">
      <Query>
        <DataSourceName>SCCM_DB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@collectionIdParameter">
            <Value>=Parameters!collectionIdParameter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          DECLARE @CICompliant int 
          DECLARE @CINotCompliant int 
          
          -- Set CI compliant value 
          SET @CICompliant = 1
          
          -- Set CI non compliant value 
          SET @CINotCompliant = 2

          SELECT 
            compliance.ComplianceStatus AS ComplianceStatus,
            compliance.ItemType AS [ItemType],
            ComputerSys.Manufacturer0 + N' ' + ComputerSys.Model0 [Hardware],
            CASE 
              WHEN MBAMPolicy.MBAMPolicyEnforced0 IS NULL THEN -1 -- (when outer join returns null : N/A)
              WHEN CompStat.ComplianceState = @CINotCompliant and MBAMPolicy.MBAMPolicyEnforced0 = 3 THEN 1 -- tmp user exempt
              WHEN CompStat.ComplianceState = @CICompliant and MBAMPolicy.MBAMPolicyEnforced0 = 2 THEN 2 -- user exempt    
              ELSE 0 -- not exempted
            END AS Exemption,
            CASE 
              WHEN CompStat.ComplianceState = @CICompliant and MBAMPolicy.MBAMPolicyEnforced0 = 0 THEN 50 -- policy not enforced
              WHEN CompStat.ComplianceState = @CICompliant and MBAMPolicy.MBAMPolicyEnforced0 = 1 THEN 0 -- no error
              WHEN CompStat.ComplianceState = @CINotCompliant and MBAMPolicy.MBAMMachineError0 IS NOT NULL and MBAMPolicy.MBAMMachineError0 &lt;&gt; 0 THEN MBAMPolicy.MBAMMachineError0 -- MBAM agent error status
              ELSE -1 -- No Details \ Errors
            END AS [ComplianceStatusDetails]
          FROM v_R_System_Valid RSystem
            INNER JOIN v_FullCollectionMembership FullCollection
              ON FullCollection.ResourceID = RSystem.ResourceID
            INNER JOIN v_SMSCICurrentComplianceStatus CompStat
              ON CompStat.ItemKey = RSystem.ResourceID
            INNER JOIN v_ConfigurationItems ConfigItems
              ON ConfigItems.ModelID = CompStat.ModelID
            INNER JOIN (SELECT ResourceID 
                  FROM v_R_System_Valid RSystem
				            INNER JOIN v_SMSCICurrentComplianceStatus CompStat
                      ON CompStat.ItemKey = RSystem.ResourceID
				            INNER JOIN v_ConfigurationItems ConfigItems
			                ON ConfigItems.ModelID = CompStat.ModelID
                    WHERE (ConfigItems.CI_ID IN (select v.CI_ID from v_BLM_CI_ID_AND_COLL_ID v where v.CollectionID = @collectionIdParameter ))) DriveTypeCompStat
              ON CompStat.ItemKey = DriveTypeCompStat.ResourceID
            INNER JOIN v_GS_COMPUTER_SYSTEM ComputerSys
              ON FullCollection.ResourceID = ComputerSys.ResourceID
            LEFT OUTER JOIN v_GS_MBAM_POLICY MBAMPolicy
              ON MBAMPolicy.ResourceID = FullCollection.ResourceID

            JOIN v_BLM_ComplianceStatus compliance
              ON compliance.CI_ID = ConfigItems.CI_ID AND compliance.ItemKey = CompStat.ItemKey

      WHERE MBAMPolicy.EncodedComputerName0 IS NOT NULL
          AND FullCollection.IsClient = 1
          AND FullCollection.CollectionID = @collectionIdParameter
          AND (ConfigItems.CI_ID IN (select v.CI_ID from v_BLM_CI_ID_AND_COLL_ID v where v.CollectionID = @collectionIdParameter))
        
		 GROUP BY MBAMPolicy.EncodedComputerName0, 
            ConfigItems.CI_UniqueID, 
            CompStat.ComplianceState, 
            ComputerSys.Manufacturer0, 
            ComputerSys.Model0, 
            MBAMPolicy.MBAMPolicyEnforced0, 
            MBAMPolicy.MBAMMachineError0,
            compliance.ComplianceStatus,
            compliance.ItemType
		UNION ALL
		SELECT CompStat.ComplianceState [ComplianceStatus],
            1 AS [ItemType],
            ComputerSys.Manufacturer0 + N' ' + ComputerSys.Model0 [Hardware],
            CASE 
              WHEN MBAMPolicy.MBAMPolicyEnforced0 IS NULL THEN -1 -- (when outer join returns null : N/A)
              WHEN CompStat.ComplianceState = @CINotCompliant and MBAMPolicy.MBAMPolicyEnforced0 = 3 THEN 1 -- tmp user exempt
              WHEN CompStat.ComplianceState = @CICompliant and MBAMPolicy.MBAMPolicyEnforced0 = 2 THEN 2 -- user exempt    
              ELSE 0 -- not exempted
            END AS Exemption,
            CASE 
              WHEN CompStat.ComplianceState = @CICompliant and MBAMPolicy.MBAMPolicyEnforced0 = 0 THEN 50 -- policy not enforced
              WHEN CompStat.ComplianceState = @CICompliant and MBAMPolicy.MBAMPolicyEnforced0 = 1 THEN 0 -- no error
              WHEN CompStat.ComplianceState = @CINotCompliant and MBAMPolicy.MBAMMachineError0 IS NOT NULL and MBAMPolicy.MBAMMachineError0 &lt;&gt; 0 THEN MBAMPolicy.MBAMMachineError0 -- MBAM agent error status
              ELSE -1 -- No Details \ Errors
            END AS [ComplianceStatusDetails]
          FROM v_R_System_Valid RSystem
            INNER JOIN v_FullCollectionMembership FullCollection
              ON FullCollection.ResourceID = RSystem.ResourceID
            INNER JOIN v_SMSCICurrentComplianceStatus CompStat
              ON CompStat.ItemKey = RSystem.ResourceID
	          INNER JOIN v_ConfigurationItems ConfigItems
			        ON ConfigItems.ModelID = CompStat.ModelID
            INNER JOIN (SELECT ResourceID 
                  FROM v_R_System_Valid RSystem
				            INNER JOIN v_SMSCICurrentComplianceStatus CompStat
                      ON CompStat.ItemKey = RSystem.ResourceID
				            INNER JOIN v_ConfigurationItems ConfigItems
			                ON ConfigItems.ModelID = CompStat.ModelID
                  WHERE (ConfigItems.CI_ID IN (select v.CI_ID from v_BLM_CI_ID_AND_COLL_ID v where v.CollectionID = @collectionIdParameter ))) DriveTypeCompStat
              ON CompStat.ItemKey = DriveTypeCompStat.ResourceID
            INNER JOIN v_GS_COMPUTER_SYSTEM ComputerSys
              ON FullCollection.ResourceID = ComputerSys.ResourceID
            LEFT OUTER JOIN v_GS_MBAM_POLICY MBAMPolicy
              ON MBAMPolicy.ResourceID = FullCollection.ResourceID
		      WHERE MBAMPolicy.EncodedComputerName0 IS NOT NULL
            AND FullCollection.IsClient = 1
            AND FullCollection.CollectionID = @collectionIdParameter
            AND (ConfigItems.CI_ID IN 
					    (select v.CI_ID from v_BLM_CI_ID_AND_COLL_ID v where v.CollectionID = @collectionIdParameter))
          GROUP BY MBAMPolicy.EncodedComputerName0, 
            ConfigItems.CI_UniqueID, 
            CompStat.ComplianceState, 
            ComputerSys.Manufacturer0, 
            ComputerSys.Model0, 
            MBAMPolicy.MBAMPolicyEnforced0, 
            MBAMPolicy.MBAMMachineError0

        </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ComplianceStatus">
          <DataField>ComplianceStatus</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ItemType">
          <DataField>ItemType</DataField>
