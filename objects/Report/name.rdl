    <DataSet Name="LCID">
      <Fields>
        <Field Name="ID">
          <DataField />
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select dbo.fn_LShortNameToLCID(@locale) </CommandText>
        <QueryParameters>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="param_ListCIs">
      <Fields>
        <Field Name="CIDisplayName">
          <DataField>CIDisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CIName">
          <DataField>CIName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CI_ID">
          <DataField>CI_ID</DataField>
          <rd:TypeName>System.Int32</rd: TypeName >
        </Field >
      </Fields >
    <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>                select distinct dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIDisplayName
        ,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIName
        ,ci.CI_ID
        from fn_rbac_ConfigurationItems(@UserSIDs)  ci
        inner join v_CIRelation_All  rel on rel.ReferencedCI_ID=ci.CI_ID and rel.RelationType &lt;&gt; 7 and rel.RelationType&lt;&gt;5 and rel.RelationType&lt;&gt;50 and rel.RelationType&lt;&gt;8 and rel.RelationType&lt;&gt;80
        inner join fn_rbac_ConfigurationItems(@UserSIDs)  bl on bl.CI_ID=rel.CI_ID and bl.CIType_ID=2
        where ci.CIType_ID not in (2,11,14) and ci.CI_ID in (Select Distinct(CI_ID) from v_CIRemediationHistory)

            order by CIDisplayName</CommandText>
        <QueryParameters>
            <QueryParameter Name="@lcid">
                <Value>=Parameters!LCID.Value</Value>
            </QueryParameter>
            <QueryParameter Name="@UserSIDs">
                <Value>=Parameters!UserSIDs.Value</Value>
            </QueryParameter>
        </QueryParameters>
    </Query>
    </DataSet >
    <DataSet Name="DataSetAdminID">
      <Fields>
        <Field Name="UserSIDs">
          <DataField>UserSIDs</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select dbo.fn_rbac_GetAdminIDsfromUserSIDs(@UserTokenSIDs) as UserSIDs</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserTokenSIDs">
            <Value>=Parameters!UserTokenSIDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet >
    <DataSet Name="main">
      <Fields>
        <Field Name="CIVersion">
          <DataField>CIVersion</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="SDMPackageVersion">
          <DataField>SDMPackageVersion</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Netbios_Name0">
          <DataField>Netbios_Name0</DataField>
          <rd:TypeName>System.String</rd: TypeName >
        </Field >
    <Field Name="UserName">
        <DataField>UserName</DataField>
        <rd:TypeName>System.String</rd: TypeName >
        </Field >
    <Field Name="FullName">
        <DataField>FullName</DataField>
        <rd:TypeName>System.String</rd: TypeName >
        </Field >
    <Field Name="UserID">
        <DataField>UserID</DataField>
        <rd:TypeName>System.Int32</rd: TypeName >
        </Field >
    <Field Name="RemediationDate">
        <DataField>RemediationDate</DataField>
        <rd:TypeName>System.DateTime</rd: TypeName >
        </Field >
    <Field Name="ComplianceState">
        <DataField>ComplianceState</DataField>
        <rd:TypeName>System.Byte</rd: TypeName >
        </Field >
    <Field Name="CIName">
        <DataField>CIName</DataField>
        <rd:TypeName>System.String</rd: TypeName >
        </Field >
    <Field Name="NumberOfRemediations">
        <DataField>NumberOfRemediations</DataField>
        <rd:TypeName>System.Int32</rd: TypeName >
        </Field >
      </Fields >
    <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT CIR.[CIVersion]
        ,CIR.[SDMPackageVersion]
        ,sys.Netbios_Name0
        ,[UserName]
        ,[FullName]
        ,[UserID]
        ,[RemediationDate]
        ,[ComplianceState]
        ,dbo.fn_GetLocalizedCIName(@LCID,cir.CI_ID) as CIName
        ,ResourceID_count as NumberOfRemediations

        FROM v_CIRemediationHistory CIR
        Join v_R_System sys on cir.ResourceID = sys.ResourceID
        join (select ResourceID, count(*) as ResourceID_count
        from v_CIRemediationHistory
        where CI_ID = @CI_CIID
        group by ResourceID) b
        on CIR.ResourceID = b.ResourceID
  where cir.CI_ID = @CI_CIID </CommandText>
        <QueryParameters>
            <QueryParameter Name="@LCID">
                <Value>=Parameters!LCID.Value</Value>
            </QueryParameter>
            <QueryParameter Name="@CI_CIID">
                <Value>=Parameters!CI_CIID.Value</Value>
            </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd: UseGenericDesigner >
      </Query >
    </DataSet >
