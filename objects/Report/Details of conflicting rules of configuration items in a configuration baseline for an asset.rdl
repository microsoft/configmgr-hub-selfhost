        <Field Name="Name">
          <Value>=SrsResources.Localization.GetDBString(Fields!UnlocCollectionName.Value, User!Language)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
        ="
              begin
              if (@filterwildcard = '')
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Name, '' as CollectionID
              UNION ALL 
              SELECT DISTINCT  Name, CollectionID FROM fn_rbac_Collection(@UserSIDs)  ORDER BY Name
              else
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Name, '' as CollectionID
              UNION ALL   
              SELECT DISTINCT  Name, CollectionID FROM fn_rbac_Collection(@UserSIDs) 
              WHERE CollectionID like @filterwildcard
              ORDER BY Name
              end
