      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          declare @allassignments table
          (
          AssignmentID int,
          CollectionID  nvarchar(512),
          CollecionName nvarchar(512),
          CI_ID       int,
          DisplayName nvarchar(512),
          CIType_ID   int,
          CI_UniqueID nvarchar(512),
          CIVersion   int
          )

          declare @AllCIs table
          (
          BL_CI_ID      int,
          CI_ID         int,
          ModelID       int,
          CIVersion     int,
          ModelName     nvarchar(512),
          CI_UniqueID   nvarchar(512),
          CIType_ID     int,
          RelationType  int,
          DisplayName   nvarchar(512)
          )

          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)

          declare @UserIDs  table (UserID int)
          insert into @UserIDs(UserID) select UserID from v_Users  where FullName = @UserName
          declare @MachineID as int
          set @MachineID = (select ResourceID from v_R_System_Valid  where Netbios_Name0 = @computer)

          insert into @allassignments (AssignmentID,CollectionID,CollecionName,CI_ID, DisplayName,CIType_ID,CI_UniqueID,CIVersion)
          select assign.AssignmentID,coll.CollectionID as CollectionID,coll.name as CollecionName,bls.CI_ID, dbo.fn_GetLocalizedCIName(@lcid,bls.CI_ID),bls.CIType_ID,bls.CI_UniqueID,bls.CIVersion
          from v_CIAssignment  assign
          inner join fn_rbac_Collection(@UserSIDs)  coll on coll.CollectionID=assign.CollectionID
          inner join v_CIAssignmentToCI  targ on targ.AssignmentID=assign.AssignmentID
          inner join v_CIRelation_All  rel on rel.CI_ID=targ.CI_ID
          inner join fn_rbac_SMSConfigurationItems(@UserSIDs)  bls on bls.CI_ID=rel.ReferencedCI_ID
          where CIType_ID in (2,50) and bls.IsTombstoned=0
          and (assign.AssignmentID = @assignmentID or @assignmentID='' or @assignmentID is NULL)
          and assign.AssignmentType in (0,8)
          and (coll.CollectionID=@collid or @collid='')
          and (dbo.fn_GetLocalizedCIName(@lcid,bls.CI_ID) = @BLname or @BLname = '')

          Insert into @AllCIs
          select distinct rel.CI_ID,rel.ReferencedCI_ID,ciref.ModelId,ciref.CIVersion,ciref.ModelName,ciref.CI_UniqueID,ciref.CIType_ID,rel.RelationType,dbo.fn_GetLocalizedCIName(@lcid,ciref.CI_ID)
          Policy from
          @allassignments bl
          inner join  v_CIRelation_All  rel on rel.CI_ID=bl.CI_ID
          inner join fn_rbac_ConfigurationItems(@UserSIDs)  ciref on ciref.CI_ID=rel.ReferencedCI_ID
          where
          ((rel.RelationType in (0, 1, 2, 3, 4)) OR (rel.RelationType = 7 and ciref.CIType_ID=50))
          and ciref.CIType_ID&lt;&gt;2 and ciref.CIType_ID &lt;&gt; 11--no baselines or global cis
          and ciref.IsTombstoned=0
          and (dbo.fn_GetLocalizedCIName(@lcid,ciref.CI_ID) = @CIname or @CIname='' or @CIName is null)
          and (ciref.CIVersion = @CIVersion or @CIVersion='' or @CIVersion is null)







        declare @RulesAndSetting table
        (
        Rule_ID int,
        Rule_CI_ID int,
        Setting_ID int,
        Setting_CI_ID int,
        RuleLogicalName nvarchar(512),
        SettingLogicalName nvarchar(512),
        RuleName nvarchar(max),
        RuleDescription nvarchar(max),
        SettingName nvarchar(max),
        SettingDescription nvarchar(max),
        SettingSourceType int,
        RuleCriteria nvarchar(max),
        Severity int
        )

        declare @countRule as int
        set @countRule = 0
        
        declare @valueRule as int
        set @valueRule = 1

        insert into @RulesAndSetting(Rule_ID,Rule_CI_ID,Setting_ID,Setting_CI_ID,RuleLogicalName,SettingLogicalName,RuleName,RuleDescription,SettingName,SettingDescription,SettingSourceType,RuleCriteria,Severity)
          select distinct
        rules.Rule_ID,
        rules.CI_ID,
        settings.Setting_ID,
        settings.CI_ID,
        rules.Rule_UniqueID,
        settings.Setting_UniqueID,
        dbo.fn_GetLocalizedProperty(@lcid,rules.RuleNameRID,rules.CI_ID) as RuleName, 
        dbo.fn_GetLocalizedProperty(@lcid,rules.RuleDescriptionRID,rules.CI_ID) as RuleDescription, 
        dbo.fn_GetLocalizedProperty(@lcid,settings.SettingNameRID,settings.CI_ID) as SettingName, 
        dbo.fn_GetLocalizedProperty(@lcid,settings.SettingDescriptionRID,settings.CI_ID) as SettingDescription,
        settings.SourceType,
        rules.Criteria,
        rules.Severity
        from @AllCIs cis 
        inner join v_CIRulesAll  allrules on allrules.CI_ID=cis.CI_ID
        inner join v_CIRules  rules on rules.Rule_ID=allrules.Rule_ID
        inner join v_CISettings  settings on settings.Setting_ID=allrules.Setting_ID
     
          DECLARE @Assignments Assignment_Type INSERT INTO @Assignments select AssignmentID from @allassignments

          select
          TargetCompliance.Netbios_Name0 as MachineName,
          TargetCompliance.FullName,
          TargetCompliance.DisplayName as ConfigurationItemName,
          rules.Severity MaxNoncomplianceCriticality,
          rules.SettingName,
          rules.SettingLogicalName,
          rules.SettingSourceType as SettingType,
          rules.SettingDescription,
          rules.RuleName as ConstraintName,
          rules.RuleLogicalName as ConstraintLogicalName,
          rules.RuleDescription as ConstraintDescription,
          case when TargetCompliance.IsRuleValue=1 then @valueRule else @countRule end as RuleType,
          rules.RuleCriteria as ValidationRule,
          rules.SettingLogicalName,
          rules.RuleLogicalName as ConstraintLogicalName,
          TargetCompliance.ContributionRulesCount as ContributionRuleCount,
          dbo.fn_GetLastAssignmentEvaluationTime(@Assignments,TargetCompliance.ResourceID,TargetCompliance.UserID) as LastComplianceMessageTime,
          TargetCompliance.CI_UniqueID,
          TargetCompliance.CIVersion,
          rules.SettingSourceType
          from 
          (
              select sys.Netbios_Name0,sys.ResourceID,users.UserID,users.FullName,AllCIs.CI_UniqueID,AllCIs.DisplayName,detail.IsRuleValue,detail.ContributionRulesCount,detail.RuleID,detail.SettingID,AllCIs.CIVersion
              from (select distinct CI_ID from @allassignments) allbl
          inner join @AllCIs AllCIs on allbl.CI_ID=AllCIs.BL_CI_ID
              cross join (select distinct resources.UserName from v_dcmdeploymentresourcesuser  resources join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID = resources.CollectionID join @allassignments allbl on allbl.AssignmentID=resources.AssignmentID and resources.UserName=@UserName) resources
              inner join v_Users  users on users.FullName=resources.UserName and users.UserID in (select UserID from @UserIDs)
              inner join v_CIComplianceStatusConflictsDetail  detail on detail.ModelID=AllCIs.ModelID and detail.CIVersion=AllCIs.CIVersion and detail.UserID=users.UserID and detail.ResourceID=@MachineID and detail.UserID in (select UserID from @UserIDs)
              inner join v_R_System_Valid  sys on sys.ResourceID=detail.ResourceID and sys.ResourceID=@MachineID
          ) TargetCompliance
          inner join @RulesAndSetting rules on rules.Rule_ID=TargetCompliance.RuleID and rules.Setting_ID=TargetCompliance.SettingID
          
          
          UNION

          select
          TargetCompliance.Netbios_Name0 as MachineName,
          TargetCompliance.FullName,
          TargetCompliance.DisplayName as ConfigurationItemName,
          rules.Severity MaxNoncomplianceCriticality,
          rules.SettingName,
          rules.SettingLogicalName,
          rules.SettingSourceType as SettingType,
          rules.SettingDescription,
          rules.RuleName as ConstraintName,
          rules.RuleLogicalName as ConstraintLogicalName,
          rules.RuleDescription as ConstraintDescription,
          case when TargetCompliance.IsRuleValue=1 then @valueRule else @countRule end as RuleType,
          rules.RuleCriteria as ValidationRule,
          rules.SettingLogicalName,
          rules.RuleLogicalName as ConstraintLogicalName,
          TargetCompliance.ContributionRulesCount as ContributionRuleCount,
          dbo.fn_GetLastAssignmentEvaluationTime(@Assignments,TargetCompliance.ResourceID,TargetCompliance.UserID) as LastComplianceMessageTime,
          TargetCompliance.CI_UniqueID,
          TargetCompliance.CIVersion,
          rules.SettingSourceType
          from 
          (   --Machine assignment
              select sys.Netbios_Name0,sys.ResourceID,users.UserID,users.FullName,AllCIs.CI_UniqueID,AllCIs.DisplayName,detail.IsRuleValue,detail.ContributionRulesCount,detail.RuleID,detail.SettingID,AllCIs.CIVersion
              from (select distinct CI_ID from @allassignments) allbl
          inner join @AllCIs AllCIs on allbl.CI_ID=AllCIs.BL_CI_ID
              cross join (select distinct cm.ResourceID from v_ClientCollectionMembers  cm join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID = cm.CollectionID join @allassignments allbl on allbl.CollectionID=cm.CollectionID and cm.ResourceID=@MachineID) cm
              inner join v_CIComplianceStatusConflictsDetail  detail on detail.ModelID=AllCIs.ModelID and detail.CIVersion=AllCIs.CIVersion and detail.ResourceID=cm.ResourceID and detail.UserID in (select UserID from @UserIDs) and detail.ResourceID=@MachineID
              inner join v_R_System_Valid  sys on sys.ResourceID=detail.ResourceID and sys.ResourceID=@MachineID
              inner join v_Users  users on users.UserID=detail.UserID and users.UserID in (select UserID from @UserIDs)
          ) TargetCompliance
          inner join @RulesAndSetting rules on rules.Rule_ID=TargetCompliance.RuleID and rules.Setting_ID=TargetCompliance.SettingID
          
          </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BLname">
            <Value>=Parameters!BLname.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CIName">
            <Value>=Parameters!CIName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CIversion">
            <Value>=Parameters!CIversion.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Computer">
            <Value>=Parameters!Computer.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UserName">
            <Value>=Parameters!UserName.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@collid">
            <Value>=Parameters!collid.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@assignmentID">
            <Value>=Parameters!assignmentID.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_DisplayName">
      <Fields>
        <Field Name="BaselineDisplayName">
          <DataField>BaselineDisplayName</DataField>
        </Field>
        <Field Name="BaselineName">
          <DataField>BaselineName</DataField>
        </Field>
        <Field Name="Baseline_UniqueID">
          <DataField>Baseline_UniqueID</DataField>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          ="
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          if (@filterwildcard = '')
          begin
          select * from
          (
              select distinct
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as BaselineDisplayName, 
              '' as BaselineName, 
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Baseline_UniqueID
              UNION ALL
              select distinct
              dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineDisplayName,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineName,  CI_UniqueID  as Baseline_UniqueID
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              where ci.CIType_ID=2
          ) temp
          order by BaselineDisplayName
          end
          else
          begin
          select * from
          (
              select distinct
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as BaselineDisplayName,
              '' as BaselineName, 
              N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as Baseline_UniqueID
              UNION ALL
              select distinct
              dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineDisplayName,dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as BaselineName,  CI_UniqueID as Baseline_UniqueID
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              where ci.CIType_ID=2      and dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) like @filterwildcard
          ) temp
          order by BaselineDisplayName
          end
          "
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_CI_DisplayName">
      <Fields>
        <Field Name="CIDisplayName">
          <DataField>CIDisplayName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="CIName">
          <DataField>CIName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="ModelName">
          <DataField>ModelName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          ="
          declare @lcid as int set @lcid = dbo.fn_LShortNameToLCID(@locale)
          if (@filterwildcard = '')
          begin
          select * from
          (
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as CIDisplayName, '' as CIName
              UNION ALL
              select distinct dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIDisplayName, dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIName
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              inner join v_CIRelation_All  rel on rel.ReferencedCI_ID=ci.CI_ID and rel.RelationType &lt;&gt; 7 and rel.RelationType&lt;&gt;5 and rel.RelationType&lt;&gt;50 and rel.RelationType&lt;&gt;8 and rel.RelationType&lt;&gt;80
              inner join fn_rbac_ConfigurationItems(@UserSIDs)  bl on bl.CI_ID=rel.CI_ID and bl.CIType_ID=2
              where ci.CIType_ID not in (2,11,14) and (dbo.fn_GetLocalizedCIName(@lcid,bl.CI_ID) = @BLname or @BLname = '')
              UNION ALL
              select distinct dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIDisplayName, dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIName
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci where ci.CIType_ID = 50 and @BLname = ''               
          ) temp
          order by CIDisplayName
          end
          else
          begin
          select * from
          (
              select N'" &amp;SrsResources.Localization.GetString("AllValues", User!Language)&amp; "' as CIDisplayName, '' as CIName
              UNION ALL
              select distinct dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIDisplayName, dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIName
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci
              inner join v_CIRelation_All  rel on rel.ReferencedCI_ID=ci.CI_ID and rel.RelationType &lt;&gt; 7 and rel.RelationType&lt;&gt;5 and rel.RelationType&lt;&gt;50 and rel.RelationType&lt;&gt;8 and rel.RelationType&lt;&gt;80
              inner join fn_rbac_ConfigurationItems(@UserSIDs)  bl on bl.CI_ID=rel.CI_ID and bl.CIType_ID=2
              where ci.CIType_ID not in (2,11,14) and (dbo.fn_GetLocalizedCIName(@lcid,bl.CI_ID) = @BLname or @BLname = '') and dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) like @filterwildcard
              UNION ALL
              select distinct dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIDisplayName, dbo.fn_GetLocalizedCIName(@lcid,ci.CI_ID) as CIName
              from fn_rbac_ConfigurationItems(@UserSIDs)  ci where ci.CIType_ID = 50 and @BLname = ''               
          ) temp
          order by CIDisplayName
          end
          "
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value />
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BLname">
            <Value>=Parameters!BLname.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_Machine">
      <Fields>
        <Field Name="MachineDisplay">
          <DataField>MachineDisplay</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="Machine">
          <DataField>Machine</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          declare @coll as varchar(max)
          if (@collid='' or @collid is NULL) set @coll = ''
          else                               set @coll = @collid
          if (@filterwildcard = '')
          select * from
          (
              select distinct SYS.Netbios_Name0 as MachineDisplay, SYS.Netbios_Name0 as Machine from  v_FullCollectionMembership_Valid  cm
              join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID = cm.CollectionID
              inner join v_R_System_Valid  sys on sys.ResourceID=cm.ResourceID
              where ((cm.CollectionID=@coll) or (@coll = ''))
          ) temp
          order by MachineDisplay
          else
          select * from
          (
              select distinct SYS.Netbios_Name0 as MachineDisplay, SYS.Netbios_Name0 as Machine from  v_FullCollectionMembership_Valid  cm
              join fn_rbac_Collection(@UserSIDs) coll on coll.CollectionID = cm.CollectionID
              inner join v_R_System_Valid  sys on sys.ResourceID=cm.ResourceID
              where ((cm.CollectionID=@coll) or (@coll = ''))
              and SYS.Netbios_Name0 like @filterwildcard
          ) temp
          order by MachineDisplay
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value>=Parameters!DeviceFilter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@collid">
            <Value>=Parameters!collid.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_DataSet_User">
      <Fields>
        <Field Name="FullDisplayName">
          <DataField>FullDisplayName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:UserDefined>true</rd:UserDefined>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AutoGen__5C6358F2_4BB6_4a1b_A16E_8D96795D8602_</DataSourceName>
        <CommandText>
          ="
          declare @CI_ID  table (CI_ID int, ModelID int, CIVersion int)
          insert into @CI_ID (CI_ID,ModelID,CIVersion)
          select cis.CI_ID,ModelID, CIVersion from fn_rbac_SMSConfigurationItems(@UserSIDs)  cis
          inner join v_LocalizedCIProperties  cilp on cilp.CI_ID = cis.CI_ID
          where (cilp.DisplayName = @CIName or @CIName = '' or @CIName is NULL)


          if (@filterwildcard = '')
          begin
          select * from
          (
              select distinct usrs.FullName as FullDisplayName, usrs.FullName
              from fn_rbac_Users(@UserSIDs) usrs
              inner join v_SMSCICurrentComplianceStatus  cics on cics.UserID=usrs.UserID
              inner join @CI_ID cis on cis.ModelID=cics.ModelID and cis.CIVersion=cics.CIVersion
              inner join v_R_System_Valid  sys on sys.ResourceID=cics.ItemKey
              where
              (sys.Netbios_Name0 = @Computer or @Computer = '' or @Computer is NULL)
          ) temp
          order by FullDisplayName
          end
          else
          begin
          select * from
          (
              select distinct usrs.FullName as FullDisplayName, usrs.FullName
              from fn_rbac_Users(@UserSIDs) usrs
              inner join v_SMSCICurrentComplianceStatus  cics on cics.UserID=usrs.UserID
              inner join @CI_ID cis on cis.ModelID=cics.ModelID and cis.CIVersion=cics.CIVersion
              inner join v_R_System_Valid  sys on sys.ResourceID=cics.ItemKey
              where
              (sys.Netbios_Name0 = @Computer or @Computer = '' or @Computer is NULL)
              and usrs.FullName like @filterwildcard
          ) temp
          order by FullDisplayName
          end
          "
        </CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="filterwildcard">
            <Value>=Parameters!UserFilter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@collid">
            <Value>=Parameters!collid.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@CIName">
            <Value>=Parameters!CIName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Computer">
            <Value>=Parameters!Computer.Value</Value>
          </QueryParameter>
